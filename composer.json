{
    "name":        "webino/webino-draw",
    "description": "XHTML Layout Renderer for Zend Framework 2",
    "type":        "library",
    "license":     "BSD-3-Clause",
    "homepage":    "http://github.com/webino/WebinoDraw",
    "keywords": [
        "xhtml", "html5", "render", "draw", "view",
        "layout", "template", "webino", "zf2", "zend", "framework"
    ],
    "authors": [
        {
            "name":     "Webino, s. r. o.",
            "email":    "info@webino.sk",
            "homepage": "http://webino.sk",
            "role":     "Developer"
        },
        {
            "name":     "Peter Bačinský",
            "email":    "peter@bacinsky.sk",
            "homepage": "http://bacinsky.sk",
            "role":     "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "zendframework/zend-di": "2.*",
        "zendframework/zend-dom": "2.*",
        "zendframework/zend-view": "2.*",
        "zendframework/zend-form": "2.*",
        "zendframework/zend-i18n": "2.*",
        "zendframework/zend-http": "2.*",
        "zendframework/zend-filter": "2.*",
        "zendframework/zend-cache": "2.*",
        "zendframework/zend-stdlib": "2.*",
        "zendframework/zend-paginator": "2.*",
        "zendframework/zend-session": "2.*",
        "zendframework/zend-eventmanager": "2.*",
        "zendframework/zend-servicemanager": "2.*"
    },
    "suggest": {
        "zendframework/zend-mvc": "To use as a Zend Framework module",
        "zendframework/zend-modulemanager": "To use as a Zend Framework module",
        "zendframework/zend-math": "To use as Zend Csrf validator",
        "zendframework/zend-i18n-resources": "Use translations"
    },
    "require-dev": {
        "webino/webino-dev": "dev-develop",
        "webino/webino-debug": "dev-develop",
        "zendframework/zend-mvc": "2.6.*",
        "zendframework/zend-modulemanager": "2.*",
        "zendframework/zend-version": "2.*",
        "zendframework/zend-console": "2.*",
        "zendframework/zend-log": "2.*",
        "zendframework/zend-math": "2.*",
        "zendframework/zend-form": "2.6.*",
        "zendframework/zend-i18n-resources": "2.*"
    },
    "autoload": {"psr-0": {"WebinoDraw": "src/"}},
    "extra": {
        "webino-devkit": {
            "context": ["module", "github", "zend"],
            "ZendSkeletonApplication": "release-2.5.0"
        }
    }
}
