<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>WebinoDraw</title>
    <parser>
        <target>._tmp/api</target>
    </parser>
    <transformer>
        <target>._api</target>
    </transformer>
    <logging>
        <level>warn</level>
        <paths>
            <default>._log/{DATE}.log</default>
            <errors>._log/{DATE}.errors.log</errors>
        </paths>
    </logging>
    <transformations>
        <template name="clean" />
    </transformations>
    <files>
        <directory>src</directory>
        <ignore>src/*/autoload_classmap.php</ignore>
    </files>
</phpdoc>
